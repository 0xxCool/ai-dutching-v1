#!/usr/bin/env python3
"""
aicli – ai-dutching-v1 command-line interface
"""
import click, os, dotenv
from rich.console import Console

dotenv.load_dotenv()
console = Console()

@click.group()
def cli():
    """AI Dutching V1 – CLI"""
    pass

@cli.command()
def ping():
    """Test ob Umgebung läuft"""
    console.print("[green]pong[/green] – .env geladen")

if __name__ == "__main__":
    cli()
